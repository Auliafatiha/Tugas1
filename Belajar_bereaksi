package oopjava;

/**
 *
 * @author LENOVO
 */
public class Mahasiswa_reaksi {
      public static void main(String[] args) {
          Mahasiswa mahasiswaku = new Mahasiswa();
          
          mahasiswaku.membaca= " sedang membaca ";
          mahasiswaku.menyontek="sedang menyontek";
          mahasiswaku.modifikasi="sedang modifikasi";
          
          System.out.println("mahasiswa " + mahasiswaku.membaca);
          System.out.println("mahasiswa " + mahasiswaku.menyontek);
          System.out.println("mahasiswa " + mahasiswaku.modifikasi);
      }
}
