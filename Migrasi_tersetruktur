package oopjava;

/**
 *
 * @author LENOVO
 */
public class Migrasi_tersetruktur {
    private int perkalian;
    
   public int getperkalian(int a , int b ){
       if(a==0 || b == 0){
           System.out.println("hasil adalah 0, coba ulangi dengan angka yang lain ");
       }
      return a * b; 
   }
}
